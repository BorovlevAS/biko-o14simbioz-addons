<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_primary_variables" inherit_id="web._assets_primary_variables">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/biko_partner_org_chart/static/src/scss/variables.scss"/>
        </xpath>
    </template>

    <template id="assets_backend" name="biko_partner_org_chart_assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/scss" href="/biko_partner_org_chart/static/src/scss/partner_org_chart.scss"/>
            <script type="text/javascript" src="/biko_partner_org_chart/static/src/js/partner_org_chart.js"/>
        </xpath>
    </template>
</odoo>
